\c 19
\p
\v 1  \w En|strong=""\w* \w ce|strong=""\w* temps-là, \w où|strong=""\w* \w il|strong=""\w* n'\w y|strong=""\w* \w avait|strong=""\w* \w pas|strong=""\w* \w de|strong=""\w* \w roi|strong=""\w* \w en|strong=""\w* \w Israël|strong=""\w*, \w il|strong=""\w* \w y|strong=""\w* \w avait|strong=""\w* \w un|strong=""\w* \w Lévite|strong=""\w* \w qui|strong=""\w* habitait \w à|strong=""\w* l'\w autre|strong=""\w* bout \w de|strong=""\w* \w la|strong=""\w* \w montagne|strong=""\w* \w d|strong=""\w*'\w Éphraïm|strong=""\w*, \w et|strong=""\w* \w qui|strong=""\w* \w prit|strong=""\w* \w pour|strong=""\w* \w lui|strong=""\w* \w une|strong=""\w* \w concubine|strong=""\w* \w de|strong=""\w* \w Bethléhem|strong=""\w* \w de|strong=""\w* \w Juda|strong=""\w*.
\v 2  \w Sa|strong=""\w* \w concubine|strong=""\w* \w se|strong=""\w* prostitua \w à|strong=""\w* \w lui|strong=""\w*, \w et|strong=""\w* s'\w en|strong=""\w* \w alla|strong=""\w* \w de|strong=""\w* \w chez|strong=""\w* \w son|strong=""\w* \w père|strong=""\w* \w à|strong=""\w* \w Bethléhem|strong=""\w* \w de|strong=""\w* \w Juda|strong=""\w*, \w où|strong=""\w* \w elle|strong=""\w* \w resta|strong=""\w* \w quatre|strong=""\w* \w mois|strong=""\w*.
\v 3  \w Son|strong=""\w* \w mari|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w et|strong=""\w* \w alla|strong=""\w* \w après|strong=""\w* \w elle|strong=""\w* \w pour|strong=""\w* \w lui|strong=""\w* \w parler|strong=""\w* \w avec|strong=""\w* bonté, \w afin|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w ramener|strong=""\w*, \w ayant|strong=""\w* \w avec|strong=""\w* \w lui|strong=""\w* \w son|strong=""\w* \w serviteur|strong=""\w* \w et|strong=""\w* \w un|strong=""\w* couple d'\w ânes|strong=""\w*. \w Elle|strong=""\w* l'amena \w dans|strong=""\w* \w la|strong=""\w* \w maison|strong=""\w* \w de|strong=""\w* \w son|strong=""\w* \w père|strong=""\w* ; \w et|strong=""\w* \w lorsque|strong=""\w* \w le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w femme|strong=""\w* \w le|strong=""\w* \w vit|strong=""\w*, \w il|strong=""\w* \w se|strong=""\w* \w réjouit|strong=""\w* \w de|strong=""\w* \w le|strong=""\w* rencontrer.
\v 4  \w Son|strong=""\w* beau-père, \w le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w fille|strong=""\w*, \w le|strong=""\w* garda \w là|strong=""\w*, \w et|strong=""\w* \w il|strong=""\w* \w resta|strong=""\w* \w avec|strong=""\w* \w lui|strong=""\w* \w trois|strong=""\w* \w jours|strong=""\w*. \w Ils|strong=""\w* \w mangèrent|strong=""\w* \w et|strong=""\w* \w burent|strong=""\w*, \w et|strong=""\w* \w restèrent|strong=""\w* \w là|strong=""\w*.
\p
\v 5  \w Le|strong=""\w* \w quatrième|strong=""\w* \w jour|strong=""\w*, \w ils|strong=""\w* \w se|strong=""\w* \w levèrent|strong=""\w* \w de|strong=""\w* \w bon|strong=""\w* \w matin|strong=""\w*, \w et|strong=""\w* \w il|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w pour|strong=""\w* \w partir|strong=""\w*. \w Le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w femme|strong=""\w* \w dit|strong=""\w* \w à|strong=""\w* \w son|strong=""\w* \w gendre|strong=""\w* : « \w Fortifie|strong=""\w* \w ton|strong=""\w* \w cœur|strong=""\w* \w avec|strong=""\w* \w un|strong=""\w* \w morceau|strong=""\w* \w de|strong=""\w* \w pain|strong=""\w*, \w et|strong=""\w* \w ensuite|strong=""\w* \w tu|strong=""\w* \w iras|strong=""\w* \w ton|strong=""\w* \w chemin|strong=""\w*. »
\v 6  \w Ils|strong=""\w* s'\w assirent|strong=""\w* \w donc|strong=""\w*, \w mangèrent|strong=""\w* \w et|strong=""\w* \w burent|strong=""\w* \w tous|strong=""\w* \w les|strong=""\w* \w deux|strong=""\w* \w ensemble|strong=""\w*. \w Le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w femme|strong=""\w* \w dit|strong=""\w* \w alors|strong=""\w* \w à|strong=""\w* l'homme : « \w Je|strong=""\w* \w te|strong=""\w* \w prie|strong=""\w* \w de|strong=""\w* \w rester|strong=""\w* \w toute|strong=""\w* \w la|strong=""\w* \w nuit|strong=""\w*, \w et|strong=""\w* \w que|strong=""\w* \w ton|strong=""\w* \w cœur|strong=""\w* \w soit|strong=""\w* \w joyeux|strong=""\w*. »
\v 7 L'\w homme|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w pour|strong=""\w* \w partir|strong=""\w*, mais \w son|strong=""\w* beau-père \w le|strong=""\w* \w pressa|strong=""\w* \w et|strong=""\w* \w il|strong=""\w* \w resta|strong=""\w* \w là|strong=""\w* \w encore|strong=""\w*.
\v 8  \w Le|strong=""\w* \w cinquième|strong=""\w* \w jour|strong=""\w*, \w il|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w de|strong=""\w* \w bon|strong=""\w* \w matin|strong=""\w* \w pour|strong=""\w* \w partir|strong=""\w* ; \w le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w femme|strong=""\w* \w lui|strong=""\w* \w dit|strong=""\w* : « \w Je|strong=""\w* \w te|strong=""\w* \w prie|strong=""\w* \w de|strong=""\w* \w fortifier|strong=""\w* \w ton|strong=""\w* \w cœur|strong=""\w* \w et|strong=""\w* \w de|strong=""\w* rester jusqu'\w à|strong=""\w* \w ce|strong=""\w* \w que|strong=""\w* \w le|strong=""\w* \w jour|strong=""\w* décline » ; \w et|strong=""\w* \w ils|strong=""\w* \w mangèrent|strong=""\w* \w tous|strong=""\w* \w les|strong=""\w* \w deux|strong=""\w*.
\p
\v 9  \w Lorsque|strong=""\w* l'\w homme|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w pour|strong=""\w* \w partir|strong=""\w*, \w lui|strong=""\w*, \w sa|strong=""\w* \w concubine|strong=""\w* \w et|strong=""\w* \w son|strong=""\w* \w serviteur|strong=""\w*, \w son|strong=""\w* beau-père, \w le|strong=""\w* \w père|strong=""\w* \w de|strong=""\w* \w la|strong=""\w* \w jeune|strong=""\w* \w femme|strong=""\w*, \w lui|strong=""\w* \w dirent|strong=""\w* : « \w Voici|strong=""\w*, \w le|strong=""\w* \w jour|strong=""\w* approche \w du|strong=""\w* \w soir|strong=""\w*, reste \w donc|strong=""\w* \w toute|strong=""\w* \w la|strong=""\w* \w nuit|strong=""\w*. \w Voici|strong=""\w* \w que|strong=""\w* \w le|strong=""\w* \w jour|strong=""\w* \w s|strong=""\w*'achève. Reste \w ici|strong=""\w*, \w pour|strong=""\w* \w que|strong=""\w* \w ton|strong=""\w* \w cœur|strong=""\w* \w soit|strong=""\w* \w joyeux|strong=""\w* ; \w et|strong=""\w* \w demain|strong=""\w*, \w pars|strong=""\w* \w de|strong=""\w* \w bonne|strong=""\w* \w heure|strong=""\w*, \w pour|strong=""\w* \w que|strong=""\w* \w tu|strong=""\w* puisses rentrer \w chez|strong=""\w* \w toi|strong=""\w*. »
\v 10 Mais l'\w homme|strong=""\w* \w ne|strong=""\w* \w voulut|strong=""\w* \w pas|strong=""\w* rester \w cette|strong=""\w* \w nuit-là|strong=""\w*, \w il|strong=""\w* \w se|strong=""\w* \w leva|strong=""\w* \w et|strong=""\w* \w alla|strong=""\w* \w près|strong=""\w* \w de|strong=""\w* \w Jébus|strong=""\w* (appelée \w aussi|strong=""\w* \w Jérusalem|strong=""\w*). \w Il|strong=""\w* avait \w avec|strong=""\w* \w lui|strong=""\w* \w un|strong=""\w* couple d'\w ânes|strong=""\w* sellés. \w Sa|strong=""\w* \w concubine|strong=""\w* \w était|strong=""\w* \w aussi|strong=""\w* \w avec|strong=""\w* \w lui|strong=""\w*.
\p
\v 11  \w Lorsqu|strong=""\w*'\w ils|strong=""\w* \w furent|strong=""\w* \w près|strong=""\w* \w de|strong=""\w* \w Jébus|strong=""\w*, \w le|strong=""\w* \w jour|strong=""\w* \w était|strong=""\w* déjà \w bien|strong=""\w* avancé, \w et|strong=""\w* \w le|strong=""\w* \w serviteur|strong=""\w* \w dit|strong=""\w* \w à|strong=""\w* \w son|strong=""\w* \w maître|strong=""\w* : « \w Viens|strong=""\w*, \w je|strong=""\w* \w te|strong=""\w* \w prie|strong=""\w*, entrons \w dans|strong=""\w* \w cette|strong=""\w* \w ville|strong=""\w* \w des|strong=""\w* \w Jébusiens|strong=""\w* \w et|strong=""\w* restons-y. »
